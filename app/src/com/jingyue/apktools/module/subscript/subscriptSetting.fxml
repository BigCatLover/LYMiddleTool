<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<AnchorPane fx:id="main" prefHeight="400.0" style="-fx-background-color: white;" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jingyue.apktools.module.subscript.SubscriptSetting">
    <children>
        <AnchorPane layoutX="0.0" prefHeight="30.0" prefWidth="500.0" style="-fx-background-color: #eeee;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Button fx:id="btnClose" mnemonicParsing="false" onAction="#close" pickOnBounds="true" prefHeight="30.0" prefWidth="34.0" styleClass="button_close" AnchorPane.rightAnchor="5" />
                <Label fx:id="title" layoutX="15.0" prefHeight="30.0" prefWidth="309.0" text="ICON和角标配置" />
            </children>
        </AnchorPane>
        <Button fx:id="btnSave" onAction="#Save" prefHeight="28.0" styleClass="button_positive" text="保存" AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="120" />
        <VBox fx:id="btnContainer" prefHeight="150.0" prefWidth="100.0" AnchorPane.leftAnchor="270" AnchorPane.topAnchor="55">
        </VBox>
        <AnchorPane fx:id="paneBorder" prefHeight="154.0" prefWidth="154.0" style="-fx-background-color: null;" styleClass="rect_line_white" AnchorPane.leftAnchor="88" AnchorPane.topAnchor="58" />
        <ImageView fx:id="icon" fitHeight="150" fitWidth="150" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="90" AnchorPane.topAnchor="60"/>
        <ImageView fx:id="subscript" fitHeight="150" fitWidth="150" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="90" AnchorPane.topAnchor="60"/>

        <Label prefWidth="150" styleClass="label_normal" text="ICON尺寸为512x512" AnchorPane.leftAnchor="90" AnchorPane.topAnchor="220" />
        <Separator prefHeight="1.0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="260" />
        <Accordion AnchorPane.leftAnchor="50" AnchorPane.topAnchor="270" AnchorPane.rightAnchor="50">
            <panes>
                <TitledPane fx:id="titlePane" text="角标配置" prefWidth="400" styleClass="title-pane"/>
            </panes>
        </Accordion>
        <Label styleClass="label_normal" alignment="CENTER_LEFT" wrapText="true" text="部分渠道在上线过程中需要使用渠道提供的角标对icon进行处理，可以根据自家icon的特点进行补贴的配置" AnchorPane.leftAnchor="50" AnchorPane.rightAnchor="50" AnchorPane.bottomAnchor="20"></Label>
    </children>
    <stylesheets>
        <URL value="@../../../../../css/common.css" />
    </stylesheets>
</AnchorPane>
